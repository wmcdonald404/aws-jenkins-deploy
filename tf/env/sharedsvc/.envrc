export AWS_PROFILE=awsprofile.wmcdonald404.sharedsvc
export AWS_ACCOUNT=$(aws sts get-caller-identity | jq -r .Account)
AWS_ENV=sharedsvc
AWS_REGION=eu-west-1
export TF_VAR_aws_account=$AWS_ACCOUNT
export TF_VAR_aws_env=$AWS_ENV
export TF_VAR_aws_region=$AWS_REGION

env_vars_required AWS_ACCOUNT

if [ -z "$AWS_ACCOUNT" ]
then
  NO_FORMAT="\033[0m"
  C_DARKORANGE3="\033[38;5;166m"
  echo -e "${C_DARKORANGE3}To resolve run: aws sso login --no-browser${NO_FORMAT}"
fi
